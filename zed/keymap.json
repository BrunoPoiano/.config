[
  {
    "context": "Editor",
    "bindings": {
      "ctrl-'": "terminal_panel::ToggleFocus"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-'": "workspace::ToggleBottomDock"
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "ctrl-tab": "pane::ActivateNextItem",
      "ctrl-shift-tab": "pane::ActivatePrevItem"
    }
  }
]
